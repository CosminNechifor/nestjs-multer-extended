{
  "name": "nest-multer-s3",
  "version": "0.0.1",
  "description": "",
  "author": "Minsung Kim",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main.js",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [
    "nestjs"
  ],
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.10.14"
  },
  "dependencies": {
    "aws-sdk": "^2.604.0",
    "multer-sharp-s3": "^0.1.0",
    "nestjs-config": "^1.4.5",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@nestjs/common": "^6.10.14",
    "@nestjs/core": "^6.10.14",
    "@nestjs/platform-express": "^6.10.14",
    "@nestjs/testing": "6.10.14",
    "@types/express": "4.17.2",
    "@types/jest": "24.9.0",
    "@types/multer": "^1.3.10",
    "@types/node": "13.1.7",
    "@types/supertest": "2.0.8",
    "jest": "24.9.0",
    "prettier": "1.19.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4",
    "supertest": "4.0.2",
    "ts-jest": "24.3.0",
    "ts-node": "8.6.2",
    "tsc-watch": "4.0.0",
    "tsconfig-paths": "3.9.0",
    "tslint": "5.20.1",
    "typescript": "3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
